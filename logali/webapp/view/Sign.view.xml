<mvc:View controllerName="logali.logali.controller.Sign"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
    <Page id="idSign" showHeader="true" title="{i18n>commonDescrTile3}" titleAlignment="Center" showFooter="true" showNavButton="false">
        <content>
        </content>
        <footer>
            <OverflowToolbar id="idOverToolSign" width="100%" height="46px" design="Auto" enabled="true" visible="true">
                <content>
                    <ToolbarSpacer id="idToolbarSign" width=""/>
                    <Button id="idSaveSign" text="Save" type="Emphasized" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false" press=""/>
                    <Button id="idCancelSign" text="Cancel" type="Default" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false" press="onNavBack"/>
                </content>
            </OverflowToolbar>
        </footer>
        <headerContent/>
        <subHeader/>
        <customHeader/>
    </Page>
</mvc:View>
